{
  "author": {
    "email": "jo3mccain@icloud.com",
    "name": "FL03",
    "url": "https://github.com/FL03"
  },
  "bugs": "https://github.com/FL03/svkcl/issues",
  "description": "SvelteKit component library for Google Maps",
  "devDependencies": {
    "@googlemaps/google-maps-services-js": "latest",
    "@sveltejs/adapter-auto": "latest",
    "@sveltejs/kit": "latest",
    "@sveltejs/package": "latest",
    "@types/google.maps": "latest",
    "autoprefixer": "latest",
    "postcss": "latest",
    "svelte": "latest",
    "svelte-check": "latest",
    "tslib": "latest",
    "typescript": "latest",
    "vite": "beta"
  },
  "exports": {
    ".": {
      "svelte": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./assets": "./dist/assets"
  },
  "files": [
    "dist",
    "dist/**/*",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "homepage": "https://github.com/FL03/svkcl/wiki",
  "license": "Apache-2.0",
  "name": "@svkcl/google-maps",
  "peerDependencies": {
    "@googlemaps/google-maps-services-js": "latest",
    "@sveltejs/kit": "latest",
    "@types/google.maps": "latest",
    "svelte": "latest"
  },
  "readme": "https://github.com/FL03/svkcl/#README",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FL03/svkcl.git"
  },
  "scripts": {
    "build": "vite build && npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev --host --port 3000",
    "package": "svelte-kit sync && svelte-package",
    "prepublishOnly": "npm run package",
    "preview": "vite preview --host --port 3000"
  },
  "svelte": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.1"
}
