	"bugs": "https://github.com/FL03/svkcl/issues",
	"devDependencies": {
		"@sveltejs/adapter-auto": "latest",
		"@sveltejs/kit": "latest",
		"@sveltejs/package": "latest",
		"@tailwindcss/forms": "latest",
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"autoprefixer": "latest",
		"eslint": "latest",
		"eslint-config-prettier": "latest",
		"eslint-plugin-svelte": "latest",
		"postcss": "latest",
		"prettier": "latest",
		"prettier-plugin-svelte": "latest",
		"publint": "latest",
		"svelte": "latest",
		"svelte-check": "latest",
		"tailwindcss": "latest",
		"tslib": "latest",
		"typescript": "latest",
		"vite": "beta"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"homepage": "https://github.com/FL03/svkcl/wiki",
	"license": "Apache-2.0",
	"name": "@pzzld/svkcl",
	"peerDependencies": {
		"svelte": "latest"
	},
	"readme": "https://github.com/FL03/svkcl/#README",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FL03/svkcl.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"version": "0.0.5"
}
